	<!--Admin-->

	<h1>Edit Admin Details</h1>
	<form class="form-horizontal" id="account-details">
		<div ng-app="grievances" ng-controller="Admin">
			<div class="form-group">
				<div class="row">
					<div class="col-sm-2 offset-sm-1">
						<label class="control-label"> Name </label>
					</div>
					<div class="col-sm-7">
						<input class="form-control" type="text" ng-model="name" readonly>
					</div>
				</div>
			</div>

			<div class="form-group">
				<div class="row">
					<div class="col-sm-2 offset-sm-1">
						<label class="control-label"> Email Id</label>
					</div>
					<div class="col-sm-7">
						<input class="form-control" type="text" ng-model="email" readonly>
					</div>
				</div>
			</div>

			<div class="form-group">
				<div class="row">
					<div class="col-sm-2 offset-sm-1">
						<label class="control-label"> Mobile Number</label>
					</div>
					<div class="col-sm-7">
						<input class="form-control" type="text" ng-model="mobile">
					</div>
				</div>
			</div>

			<div class="form-group">

				<div class="col-sm-9 offset-sm-3">
					<button class="btn btn-default" type="button" id="Admin-details" ng-click="submit()" >Update</button>
				</div>

			</div>

		</div>

	</form>

        <div id="loading">
            <div class="loader">
            </div>
            
            <h1>Loading...</h1>
        </div>
	<!--Gcm-->

	<h1>Edit Member Details</h1>
	<form class="form-horizontal">
		<div ng-app="grievances" ng-controller="Gcm">
			<div class="form-group">
				<div class="row">
					<div class="col-sm-2 col-sm-offset-1">
						<label class="control-label"> Name </label>
					</div>
					<div class="col-sm-7">
						<input class="form-control" type="text" ng-model="name" readonly>
					</div>
				</div>
			</div>

			<div class="form-group">
				<div class="row">
					<div class="col-sm-2 col-sm-offset-1">
						<label class="control-label">Designation</label>
					</div>
					<div class="col-sm-7">
						<input class="form-control" type="text" ng-model="Desg" readonly>
					</div>
				</div>
			</div>

			<div class="form-group">
				<div class="row">
					<div class="col-sm-2 col-sm-offset-1">
						<label class="control-label">Grievance Type</label>
					</div>
					<div class="col-sm-7">
						<input class="form-control" type="text" ng-model="Gtype" readonly>
					</div>
				</div>
			</div>

			<div class="form-group">
				<div class="row">
					<div class="col-sm-2 col-sm-offset-1">
						<label class="control-label"> Email Id</label>
					</div>
					<div class="col-sm-7">
						<input class="form-control" type="text" ng-model="email" readonly>
					</div>
				</div>
			</div>

			<div class="form-group">
				<div class="row">
					<div class="col-sm-2 col-sm-offset-1">
						<label class="control-label"> Mobile Number</label>
					</div>
					<div class="col-sm-7">
						<input class="form-control" type="text" ng-model="mobile" readonly>
					</div>
				</div>
			</div>

			<div class="form-group">

				<div class="col-sm-9 col-sm-offset-3">
					<button class="btn btn-default" type="submit" id="student-details">Update</button>
				</div>

			</div>

		</div>

	</form>

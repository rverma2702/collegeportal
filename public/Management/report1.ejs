<h1>Consolidated Grievance Redressal Report</h1>
<div class="content" ng-app="grievances" ng-controller="Management_grievances_report">
<div class="form-group">
 <div class="row">
    <div class="col-sm-2 offset-sm-1">
     <label class="control-label"> From Date</label>
 </div>
 <div class="col-sm-7">
     <input class="form-control" type="date" ng-model="from-date" >
 </div>
</div>
</div>
<div class="form-group">
    <div class="row">
        <div class="col-sm-2 offset-sm-1">
         <label class="control-label"> To Date</label>
     </div>
     <div class="col-sm-7">
         <input class="form-control" type="date" ng-model="to-date" >
     </div>
 </div>
</div>
<div class="form-group">
    <div class="col-sm-7 offset-sm-3">
        <button class="btn btn-default" type="submit" id="student-details" ng-click="consolidate()" >Update</button>
 </div>
</div>
</div>